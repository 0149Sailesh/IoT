<body>
  <div class="ui inverted menu" style="position:absolute;">
  <a class="item" href="/">
	Login
  </a>
  <a class="item" href="/register">
    Sign Up
  </a>
</div>
<canvas></canvas>
<div class="login-box" style="position:absolute;">
  <h1>Sign Up</h1>
	<form action="/register" method="POST">
		
		 <div class="textbox">
    <i class="fas fa-user"></i>
    <input type="text" placeholder="Username" name="username">
  </div>

  <div class="textbox">
    <i class="fas fa-lock"></i>
    <input type="password" placeholder="Password" name="password">
  </div>
		<div class="textbox">
    <i class="fa fa-key"></i>
    <input type="text" placeholder="Key" name="admin">
  </div>
		<div class="textbox">
    <i class="fas fa-envelope"></i>
    <input type="email" placeholder="Email" name="email">
  </div>
  <button class="btn">
	  Sign Up
		</button>

	</form>
	  </div>
	
	
	
